{
  "name": "react-starter-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:css": "tailwindcss -m -i src/styles/tailwind.css -o src/styles/app.css",
    "build:vite": "vite",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:css": "tailwindcss -w -i src/styles/tailwind.css -o src/styles/app.css",
    "dev:vite": "vite",
    "preview": "vite preview",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --report-unused-disable-directives --max-warnings 0 --fix",
    "prettify": "prettier src --check",
    "prettify:fix": "prettier src --write",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "chess.js": "1.0.0-beta.6",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-chessboard": "^4.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "socket.io-client": "^4.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.196",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "daisyui": "^3.5.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "postcss": "^8.4.27",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
